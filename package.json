{
  "name": "akshon",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "gen": "tsc --project tsconfig.json",
    "build": "jsii",
    "build:watch": "jsii --watch",
    "package": "jsii-pacmak",
    "pack": "jsii-pacmak --code-only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Loag/akshon.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [],
  "author": {
    "name": "Loag"
  },
  "license": "Apache-2.0",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Loag/akshon/issues"
  },
  "homepage": "https://github.com/Loag/akshon#readme",
  "stability": "stable",
  "types": "lib/index.d.ts",
  "jsii": {
    "versionFormat": "full",
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "org.akshon",
        "maven": {
          "groupId": "org.akshon",
          "artifactId": "akshon"
        }
      },
      "dotnet": {
        "namespace": "Org.Akshon",
        "packageId": "Org.Akshon"
      },
      "python": {
        "distName": "akshon",
        "module": "akshon"
      },
      "go": {
        "moduleName": "github.com/Loag/akshon-go"
      }
    },
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  },
  "dependencies": {
    "constructs": "^10.4.3",
    "yaml": "^2.0.0"
  },
  "bundledDependencies": [
    "yaml"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0",
    "jsii": "^5.9.13",
    "jsii-pacmak": "^1.119.0"
  }
}